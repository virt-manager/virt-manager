        </interface>
        <interface type='ethernet' name='brbond-eth1'>
        </interface>
      </bond>
    </interface>
    <interface type='ethernet' name='eth0'>
      <start mode='none'/>
      <mac address='00:FF:EE:FF:EE:FF'/>
    </interface>
  </bridge>
